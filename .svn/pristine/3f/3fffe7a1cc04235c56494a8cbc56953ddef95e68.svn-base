{% extends "index.html" %}
{% block first %}
<div class="jumbotron">
	<h2>Changelog</h2>
</div>

<div>
	<h3>v 0.2</h3>
	<table class="table table-striped table-hover table-bordered">
		<thead>
    		<tr class="info">
        		<th>Type</th>
          	  	<th>Description</th>
        	</tr>
        </thead> 
        <tbody>
        	<tr>
				<td><span class="badge badge-info">fix</span></td>
