    url(r'^signup/$', forms.register, name='register'),
    # Examples:
    # url(r'^$', 'ramco.views.home', name='home'),
    # url(r'^ramco/', include('ramco.foo.urls')),

    # Uncomment the admin/doc line below to enable admin documentation:
